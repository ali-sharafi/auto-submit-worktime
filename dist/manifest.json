{
	"manifest_version": 3,
	"name": "Translator",
	"description": "Add your new vocabulary to learning docs",
	"version": "1.0",
	"background": {
		"service_worker": "/js/main.js"
	},
	"content_scripts": [
		{
			"all_frames": true,
			"matches": [
				"http://*/*",
				"https://*/*"
			],
			"js": [
				"/js/app.js"
			],
			"css": [
				"/css/app.css"
			]
		}
	],
	"permissions": [
		"identity",
		"scripting"
	],
	"host_permissions": [
		"*://*/*"
	],
	"action": {
		"default_icon": "icon.png",
		"default_title": "Translator"
	},
	"oauth2": {
		// "client_id": "932312406121-pu5i9cd7odo0gvp55gpr9t9eh28seiu0.apps.googleusercontent.com",
		"client_id": "932312406121-u95lkeo6p30hjil97mk9liskjcrs5r28.apps.googleusercontent.com",
		"scopes": [
			"https://www.googleapis.com/auth/spreadsheets",
			"https://www.googleapis.com/auth/drive",
			"https://www.googleapis.com/auth/drive.readonly",
			"https://www.googleapis.com/auth/drive.file",
			"https://www.googleapis.com/auth/spreadsheets.readonly"
		]
	}
}